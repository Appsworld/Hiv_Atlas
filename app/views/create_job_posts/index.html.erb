    <td><%= link_to 'Home', profiles_path %></td>
    <td><%= link_to 'Show', create_job_post %></td>
    <td><%= link_to 'Edit', edit_create_job_post_path(create_job_post) %></td>
    <td><%= link_to 'Destroy', create_job_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
