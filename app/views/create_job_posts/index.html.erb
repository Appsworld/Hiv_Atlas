<% @create_job_posts.each do |create_job_post| %>
  <tr>
    <td><%= create_job_post.user_id %></td>
    <td><%= create_job_post.job_title %></td>
    <td><%= create_job_post.email %></td>
    <td><%= create_job_post.enabled %></td>
